#include<stdio.h>
#include<stdlib.h>
struct node{
int key;

struct node{
int key;
struct node * right;
struct node * left;
};

void main(){

    //INSERT A TREE

    if(isvalid(root,-10000,10000)){
        printf("\nValid BST!");
    }
    else{
        printf("\nInvalid BST!");
    }
}

int isvalid(struct node * root,int min ,int max){
    if(root==NULL){
        return 1;
    }
    if(root->key<=min||root->key>=max){
        return 0;
    }
    return isvalid(root->left,min,root->key)&&isvalid(root->right,root->key,max);
    
}
