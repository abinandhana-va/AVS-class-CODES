import java.util.*;
class Main{
    public static void main(String args[]){
        int n=10000;
        System.out.println("ArrayList : ");
        ArrayList<Integer> arr=new ArrayList<>();
        long start=System.nanoTime();
        for(int i=0;i<n;i++){
            arr.add(0,i);
        }
        long end=System.nanoTime();
        long diff=end-start;
        System.out.println("Starting Second: "+start);
        System.out.println("Ending second : "+end);
        System.out.println("Diff :"+diff);
        
        System.out.println("LinkedList :");
        LinkedList<Integer> ll=new LinkedList<>();
        long startl=System.nanoTime();
        for(int j=0;j<n;j++){
            ll.add(0,j);
        }
        long endl=System.nanoTime();
        long diffl=endl-startl;
        System.out.println("Starting Second: "+start);
        System.out.println("Ending second : "+end);
        System.out.println("Diff :"+diff);
        if(diff>diffl){
            System.out.println("LinkedList is Better");
        }
        else{
            System.out.println("ArrayList is Better");
        }
    }
}
